# Ignore bundler config.
/fb/.bundle

# Ignore the default SQLite database.
/fb/db/*.sqlite3
/fb/db/*.sqlite3-journal
/fb/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/fb/log/*
/fb/tmp/*
/fb!/log/.keep
/fb!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/fb/tmp/pids/*
/fb!/tmp/pids/
/fb!/tmp/pids/.keep

# Ignore uploaded files in development.
/fb/storage/*
/fb!/storage/.keep

/fb/public/assets
/fb.byebug_history

# Ignore master key for decrypting credentials and more.
/fb/config/master.key

/fb/public/packs
/fb/public/packs-test
/fb/node_modules
/fb/yarn-error.log
/fbyarn-debug.log*
/fb.yarn-integrity


# Ignore Spring files.
/fb/spring/*.pid